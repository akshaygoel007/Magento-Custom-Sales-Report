                    <item name="labelField" xsi:type="string">increment_order_id</item>
                    <item name="actionType" xsi:type="string">view</item>
                    <item name="sendStoreId" xsi:type="string">false</item>
                    <item name="label" xsi:type="string" translate="true">Order ID</item>
